{
  "manifest_version": 2,
  "name": "URLify",
  "version": "1.0",
  "description": "Converts 'DOT' in URLs to '.' and makes them clickable",
  "permissions": [ "activeTab", "storage", "<all_urls>" ],
  "content_scripts": [
    {
      "matches": [ "<all_urls>" ],
      "js": [ "content-script.js" ]
    }
  ]
}